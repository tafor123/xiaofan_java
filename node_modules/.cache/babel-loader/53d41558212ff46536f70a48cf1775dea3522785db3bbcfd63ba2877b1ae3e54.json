{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'BookList',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableDate: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        name: '',\n        book_no: ''\n      },\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入新的图书名称',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 10,\n          message: '长度在3到10个字符',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      // fetch('http://localhost:9090/book/list').then(res => res.json()).then(res =>{\n      //   console.log(res)\n      //   this.tableDate=res\n      // })\n\n      request.get('/book/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableDate = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        name: '',\n        book_no: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      //点击分页按钮触发\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete(\"/book/delete/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$notify.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookies","name","data","admin","get","JSON","parse","tableDate","total","params","pageNum","pageSize","book_no","rules","required","message","trigger","min","max","created","load","methods","then","res","code","list","reset","handleCurrentChange","del","id","delete","$notify","success","error","msg"],"sources":["src/views/book/List.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!--  搜索表单  -->\r\n    <div style=\"margin-bottom: 20px\">\r\n      <el-input style=\"width: 240px\" placeholder=\"请输入图书名称\" v-model=\"params.name\"></el-input>\r\n      <el-input style=\"width: 240px; margin-left: 5px\" placeholder=\"请输入图书标准码\" v-model=\"params.book_no\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"> <i class=\"el-icon-search\"></i>搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"> <i class=\"el-icon-refresh\"></i>重置</el-button>\r\n    </div>\r\n\r\n    <el-table :data=\"tableDate\" stripe row-key=\"id\"  default-expand-all>\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"图书\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"description\" label=\"描述\" width=\"200\"></el-table-column>\r\n      <el-table-column prop=\"publish_date\" label=\"出版时间\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"author\" label=\"作者\" width=\"50\"></el-table-column>\r\n      <el-table-column prop=\"publish\" label=\"出版社\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"category\" label=\"分类\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"book_no\" label=\"标准码\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"cover\" label=\"封面\" width=\"50\">\r\n        <template v-slot=\"scope\">\r\n          el\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"createtime\" label=\"创建时间\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"updatetime\" label=\"更新时间\" width=\"100\"></el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template v-slot=\"scope\">\r\n<!--          scope.row就是当前行-->\r\n          <el-button type=\"success\" @click=\"$router.push('/editBook?id=' + scope.row.id)\">编辑</el-button>\r\n          <el-popconfirm\r\n              style=\"margin-left: 4px\"\r\n              title=\"你确定删除这行数据吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n\r\n    </el-table>\r\n\r\n    <!--  分页  -->\r\n    <div style=\"margin-top: 20px\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie'\r\nexport default {\r\n  name: 'BookList',\r\n  data(){\r\n    return{\r\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\r\n      tableDate: [],\r\n      total: 0,\r\n      params:{\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        name: '',\r\n        book_no: ''\r\n      },\r\n      rules: {\r\n        name:[\r\n          {required: true, message: '请输入新的图书名称', trigger: 'blur'},\r\n          {min: 3, max: 10, message:'长度在3到10个字符', trigger: 'blur'}\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load(){\r\n      // fetch('http://localhost:9090/book/list').then(res => res.json()).then(res =>{\r\n      //   console.log(res)\r\n      //   this.tableDate=res\r\n      // })\r\n\r\n      request.get('/book/page',{\r\n        params: this.params\r\n      }).then(res =>{\r\n        if(res.code === '200'){\r\n          this.tableDate = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.params={\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        name: '',\r\n        book_no: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      //点击分页按钮触发\r\n      this.params.pageNum=pageNum\r\n      this.load()\r\n    },\r\n    del(id){\r\n      request.delete(\"/book/delete/\" +id).then(res =>{\r\n        if(res.code === '200'){\r\n          this.$notify.success(\"删除成功\")\r\n          this.load()\r\n        }else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA2DA,OAAAA,OAAA;AACA,OAAAC,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA,EAAAH,OAAA,CAAAI,GAAA,YAAAC,IAAA,CAAAC,KAAA,CAAAN,OAAA,CAAAI,GAAA;MACAG,SAAA;MACAC,KAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAV,IAAA;QACAW,OAAA;MACA;MACAC,KAAA;QACAZ,IAAA,GACA;UAAAa,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAAA;UAAAC,GAAA;UAAAH,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAG,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA;MACA;MACA;MACA;;MAEArB,OAAA,CAAAK,GAAA;QACAK,MAAA,OAAAA;MACA,GAAAa,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAjB,SAAA,GAAAgB,GAAA,CAAArB,IAAA,CAAAuB,IAAA;UACA,KAAAjB,KAAA,GAAAe,GAAA,CAAArB,IAAA,CAAAM,KAAA;QACA;MACA;IACA;IACAkB,MAAA;MACA,KAAAjB,MAAA;QACAC,OAAA;QACAC,QAAA;QACAV,IAAA;QACAW,OAAA;MACA;MACA,KAAAQ,IAAA;IACA;IACAO,oBAAAjB,OAAA;MACA;MACA,KAAAD,MAAA,CAAAC,OAAA,GAAAA,OAAA;MACA,KAAAU,IAAA;IACA;IACAQ,IAAAC,EAAA;MACA9B,OAAA,CAAA+B,MAAA,mBAAAD,EAAA,EAAAP,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAO,OAAA,CAAAC,OAAA;UACA,KAAAZ,IAAA;QACA;UACA,KAAAW,OAAA,CAAAE,KAAA,CAAAV,GAAA,CAAAW,GAAA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}