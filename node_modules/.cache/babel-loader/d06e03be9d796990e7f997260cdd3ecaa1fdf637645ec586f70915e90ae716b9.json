  {required: true, message: '请输入图书名称', trigger: 'blur'},\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created(){\r\n    request.get('/book/list').then(res =>{\r\n      this.books=res.data\r\n    })\r\n\r\n    request.get('/user/list').then(res =>{\r\n      this.users=res.data\r\n    })\r\n  },\r\n  methods: {\r\n    save(){\r\n      this.$refs['ruleForm'].validate((valid) =>{\r\n        if (valid) {\r\n          request.post('/book/save',this.form).then(res => {\r\n            if (res.code === '200'){\r\n              this.$notify.success('新增成功')\r\n              this.$refs['ruleForm'].resetFields()\r\n            }else{\r\n              this.$notify.error(res.msg)\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    selBook(){\r\n     const book = this.books.find(v =>v.book_no === this.form.book_no)\r\n     this.form.book_name = book.name\r\n     this.form.score = book.score\r\n    },\r\n    selUser(){\r\n      const user=this.users.find(v =>v.username === this.form.user_no)\r\n      this.form.user_name = user.name\r\n      this.form.user_phone = user.phone\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA+CA,OAAAA,OAAA;AACA,OAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,KAAA;MACAC,KAAA;MACAC,KAAA;QACAL,IAAA,GACA;UAAAM,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,QAAA;IACAX,OAAA,CAAAY,GAAA,eAAAC,IAAA,CAAAC,GAAA;MACA,KAAAT,KAAA,GAAAS,GAAA,CAAAX,IAAA;IACA;IAEAH,OAAA,CAAAY,GAAA,eAAAC,IAAA,CAAAC,GAAA;MACA,KAAAR,KAAA,GAAAQ,GAAA,CAAAX,IAAA;IACA;EACA;EACAY,OAAA;IACAC,KAAA;MACA,KAAAC,KAAA,aAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACAnB,OAAA,CAAAoB,IAAA,oBAAAhB,IAAA,EAAAS,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAO,IAAA;cACA,KAAAC,OAAA,CAAAC,OAAA;cACA,KAAAN,KAAA,aAAAO,WAAA;YACA;cACA,KAAAF,OAAA,CAAAG,KAAA,CAAAX,GAAA,CAAAY,GAAA;YACA;UACA;QACA;MACA;IACA;IACAC,QAAA;MACA,MAAAC,IAAA,QAAAvB,KAAA,CAAAwB,IAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAC,OAAA,UAAA3B,IAAA,CAAA2B,OAAA;MACA,KAAA3B,IAAA,CAAA4B,SAAA,GAAAJ,IAAA,C