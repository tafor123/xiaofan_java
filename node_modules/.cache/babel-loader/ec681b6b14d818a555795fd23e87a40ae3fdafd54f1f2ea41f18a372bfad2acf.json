{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from \"js-cookie\";\nexport default {\n  name: \"AddBorrow\",\n  data() {\n    return {\n      form: {},\n      books: [],\n      users: [],\n      rules: {\n        book_no: [{\n          required: true,\n          message: '请输入图书标准码',\n          trigger: 'blur'\n        }],\n        user_no: [{\n          required: true,\n          message: '请输入会员码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    request.get('/book/list').then(res => {\n      this.books = res.data;\n    });\n    request.get('/user/list').then(res => {\n      this.users = res.data.filter(v => v.status);\n    });\n  },\n  methods: {\n    save() {\n      this.$refs['ruleForm'].validate(valid => {\n        if (valid) {\n          request.post('/borrow/save', this.form).then(res => {\n            if (res.code === '200') {\n              this.$notify.success('新增成功');\n              this.$refs['ruleForm'].resetFields();\n            } else {\n              this.$notify.error(res.msg);\n            }\n          });\n        }\n      });\n    },\n    selBook() {\n      const book = this.books.find(v => v.book_no === this.form.book_no);\n      request.get('/book/' + book.id).then(res => {\n        this.$set(this.form, 'book_name', res.data.name);\n        this.form.score = res.data.score;\n        this.form.nums = res.data.nums;\n      });\n    },\n    selUser() {\n      const user = this.users.find(v => v.username === this.form.user_no);\n      request.get('/user/' + user.id).then(res => {\n        this.$set(this.form, 'user_name', res.data.name);\n        this.form.user_phone = res.data.phone;\n        this.form.account = res.data.account;\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookies","name","data","form","books","users","rules","book_no","required","message","trigger","user_no","created","get","then","res","filter","v","status","methods","save","$refs","validate","