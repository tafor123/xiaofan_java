el-input v-model=\"form.user_phone\" disabled></el-input>\r\n     </el-form-item>\r\n     <el-form-item label=\"用户账户积分\" prop=\"account\">\r\n       <el-input v-model=\"form.account\" disabled></el-input>\r\n     </el-form-item>\r\n     <el-form-item label=\"借出天数\" prop=\"days\">\r\n       <el-input-number v-model=\"form.days\"  :min=\"1\" :max=\"30\" label=\"所用积分\"></el-input-number>\r\n     </el-form-item>\r\n\r\n   </el-form>\r\n\r\n   <div style=\"text-align: center; margin-top: 30px\">\r\n     <el-button type=\"primary\" @click=\"save\" size=\"medium\">提交</el-button>\r\n<!--     <el-button type=\"danger\">返回列表</el-button>-->\r\n   </div>\r\n </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from \"js-cookie\";\r\n\r\nexport default {\r\n  name: \"AddBorrow\",\r\n  data(){\r\n    return {\r\n      form: {},\r\n      books: [],\r\n      users:[],\r\n      rules: {\r\n        book_no: [\r\n          {required: true, message: '请输入图书标准码', trigger: 'blur'},\r\n        ],\r\n        user_no: [\r\n          {required: true, message: '请输入会员码', trigger: 'blur'},\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created(){\r\n    request.get('/book/list').then(res =>{\r\n      this.books=res.data\r\n    })\r\n\r\n    request.get('/user/list').then(res =>{\r\n      this.users=res.data.filter(v => v.status)\r\n    })\r\n  },\r\n  methods: {\r\n    save(){\r\n      this.$refs['ruleForm'].validate((valid) =>{\r\n        if (valid) {\r\n          request.post('/borrow/save',this.form).then(res => {\r\n            if (res.code === '200'){\r\n              this.$notify.success('新增成功')\r\n              this.$refs['ruleForm'].resetFields()\r\n            }else{\r\n              this.$notify.error(res.msg)\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    selBook(){\r\n      const book = this.books.find(v =>v.book_no === this.form.book_no)\r\n      request.get('/book/'+book.id).then(res =>{\r\n        this.$set(this.form,'book_name',res.data.name)\r\n        this.form.score = res.data.score\r\n        this.form.nums = res.data.nums\r\n      })\r\n    },\r\n    selUser(){\r\n      const user=this.users.find(v =>v.username === this.form.user_no)\r\n      request.get('/user/'+user.id).then(res =>{\r\n        this.$set(this.form,'user_name',res.data.name)\r\n        this.form.user_phone = res.data.phone\r\n        this.form.account = res.data.account\r\n      })\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAyDA,OAAAA,OAAA;AACA,OAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,KAAA;MACAC,KAAA;MACAC,KAAA;QACAC,OAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAC,OAAA,GACA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAE,QAAA;IACAb,OAAA,CAAAc,GAAA,eAAAC,IAAA,CAAAC,GAAA;MACA,KAAAX,KAAA,GAAAW,GAAA,CAAAb,IAAA;IACA;IAEAH,OAAA,CAAAc,GAAA,eAAAC,IAAA,CAAAC,GAAA;MACA,KAAAV,KAAA,GAAAU,GAAA,CAAAb,IAAA,CAAAc,MAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAC,MAAA;IACA;EACA;EACAC,OAAA;IACAC,KAAA;MACA,KAAAC,KAAA,aAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACAxB,OAAA,CAAAyB,IAAA,sBAAArB,IAAA,EAAAW,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAU,IAAA;cACA,KAAAC,OAAA,CAAAC,OAAA;cACA,KAAAN,KAAA,aAAAO,WAAA;YACA;cACA,KAAAF,OAAA,CAAAG,KAAA,CAAAd,GAAA,CAAAe,GAAA;YACA;UACA;QACA;MACA;IACA;IACAC,QAAA;MACA,MAAAC,IAAA,QAAA5B,KAAA,CAAA6B,IAAA,CAAAhB,CAAA,IAAAA,CAAA,CAAAV,OAAA,UAAAJ,IAAA,CAAAI,OAAA;MACAR,OAAA,CAAAc,GAAA,YAAAmB,IAAA,CAAAE,EAAA,EAAApB,IAAA,CAAAC,GAAA;QACA,KAAAoB,IAAA,MAAAhC,IAAA,eAAAY,GAAA,CAAAb,IAAA,CAAAD,IAAA;QACA,KAAAE,IAAA,CAAAiC,KAAA,GAAArB,GAAA,CAAAb,IAAA,CAAAkC,KAAA;QACA,KAAAjC,IAAA,CAAAkC,IAAA,GAAAtB,GAAA,CAAAb,IAAA,CAAAmC,IAAA;MACA;IACA;IACAC,QAAA;MACA,MAAAC,IAAA,QAAAlC,KAAA,CAAA4B,IAAA,CAAAhB,CAAA,IAAAA,CAAA,CAAAuB,QAAA,UAAArC,IAAA,CAAAQ,OAAA;MACAZ,OAAA,CAAAc,GAAA,YAAA0B,IAAA,CAAAL,EAAA,EAAApB,IAAA,CAAAC,GAAA;QACA,KAAAoB,IAAA,MAAAhC,IAAA,eAAAY,GAAA,CAAAb,IAAA,CAAAD,IAAA;QACA,KAAAE,IAAA,CAAAsC,UAAA,GAAA1B,GAAA,CAAAb,IAAA,CAAAwC,KAAA;QACA,KAAAvC,