{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'CategoryList',\n  data() {\n    return {\n      category: Cookies.get('category') ? JSON.parse(Cookies.get('category')) : {},\n      tableDate: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        name: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      // fetch('http://localhost:9090/category/list').then(res => res.json()).then(res =>{\n      //   console.log(res)\n      //   this.tableDate=res\n      // })\n\n      request.get('/category/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableDate = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        name: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      //点击分页按钮触发\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete(\"/category/delete/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$notify.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookies","name","data","category","get","JSON","parse","tableDate","total","params","pageNum","pageSize","created","load","methods","then","res","code","list","reset","handleCurrentChange","del","id","delete","$notify","success","error","msg"],"sources":["src/views/category/List.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!--  搜索表单  -->\r\n    <div style=\"margin-bottom: 20px\">\r\n      <el-input style=\"width: 240px\" placeholder=\"请输入分类名称\" v-model=\"params.username\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"> <i class=\"el-icon-search\"></i>搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"> <i class=\"el-icon-refresh\"></i>重置</el-button>\r\n    </div>\r\n\r\n    <el-table :data=\"tableDate\" stripe row-key=\"id\"  default-expand-all>\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"名称\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"remark\" label=\"备注\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"createtime\" label=\"创建时间\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"updatetime\" label=\"更新时间\" width=\"100\"></el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template v-slot=\"scope\">\r\n<!--          scope.row就是当前行-->\r\n          <el-button type=\"primary\" @click=\"$router.push('/editCategory?id=' + scope.row.id)\">编辑</el-button>\r\n          <el-button type=\"primary\" @click=\"\" >添加二级分类</el-button>\r\n          <el-popconfirm\r\n              style=\"margin-left: 4px\"\r\n              title=\"你确定删除这行数据吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n\r\n    </el-table>\r\n\r\n    <!--  分页  -->\r\n    <div style=\"margin-top: 20px\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie'\r\nexport default {\r\n  name: 'CategoryList',\r\n  data(){\r\n    return{\r\n      category: Cookies.get('category') ? JSON.parse(Cookies.get('category')) : {},\r\n      tableDate: [],\r\n      total: 0,\r\n      params:{\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        name: ''\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load(){\r\n      // fetch('http://localhost:9090/category/list').then(res => res.json()).then(res =>{\r\n      //   console.log(res)\r\n      //   this.tableDate=res\r\n      // })\r\n\r\n      request.get('/category/page',{\r\n        params: this.params\r\n      }).then(res =>{\r\n        if(res.code === '200'){\r\n          this.tableDate = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.params={\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        name: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      //点击分页按钮触发\r\n      this.params.pageNum=pageNum\r\n      this.load()\r\n    },\r\n    del(id){\r\n      request.delete(\"/category/delete/\" +id).then(res =>{\r\n        if(res.code === '200'){\r\n          this.$notify.success(\"删除成功\")\r\n          this.load()\r\n        }else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAiDA,OAAAA,OAAA;AACA,OAAAC,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA,EAAAH,OAAA,CAAAI,GAAA,eAAAC,IAAA,CAAAC,KAAA,CAAAN,OAAA,CAAAI,GAAA;MACAG,SAAA;MACAC,KAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAV,IAAA;MACA;IACA;EACA;EACAW,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA;MACA;MACA;MACA;;MAEAd,OAAA,CAAAK,GAAA;QACAK,MAAA,OAAAA;MACA,GAAAM,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAV,SAAA,GAAAS,GAAA,CAAAd,IAAA,CAAAgB,IAAA;UACA,KAAAV,KAAA,GAAAQ,GAAA,CAAAd,IAAA,CAAAM,KAAA;QACA;MACA;IACA;IACAW,MAAA;MACA,KAAAV,MAAA;QACAC,OAAA;QACAC,QAAA;QACAV,IAAA;MACA;MACA,KAAAY,IAAA;IACA;IACAO,oBAAAV,OAAA;MACA;MACA,KAAAD,MAAA,CAAAC,OAAA,GAAAA,OAAA;MA