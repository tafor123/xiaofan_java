{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'AdminList',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableDate: [],\n      total: 0,\n      form: {},\n      dialogFormVisible: false,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        username: '',\n        phone: '',\n        email: ''\n      },\n      rules: {\n        newPass: [{\n          required: true,\n          message: '请输入新密码',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 10,\n          message: '长度在3到10个字符',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    changeStatus(row) {\n      if (this.admin.id === row.id) {\n        this.$notify.success(\"你的操作不合法\");\n        return;\n      }\n      request.put('/admin/update', row).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('操作成功');\n          this.load();\n        } else {\n          this.$notify.success(res.msg);\n        }\n      });\n    },\n    handleChangePass(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogFormVisible = true;\n    },\n    savePass() {\n      this.$refs['formRef'].validate(valid => {\n        if (valid) {\n          request.put('/admin/password', this.form).then(res => {\n            if (res.code === '200') {\n              this.$notify.success(\"修改成功\");\n              if (this.form.id === this.admin.id) {\n                //修改当前用户的ID等于用户的ID，重新登录\n                Cookies.remove('admin');\n                this.$router.push('/login');\n              } else {\n                this.load();\n                this.dialogFormVisible = false;\n              }\n            } else {\n              this.$notify.error(\"修改失败\");\n            }\n          });\n        }\n      });\n    },\n    load() {\n      // fetch('http://localhost:9090/admin/list').then(res => res.json()).then(res =>{\n      //   console.log(res)\n      //   this.tableDate=res\n      // })\n\n      request.get('/admin/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableDate = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        username: '',\n        phone: '',\n        email: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      //点击分页按钮触发\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete(\"/admin/delete/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$notify.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookies","name","data","admin","get","JSON","parse","tableDate","total","form","dialogFormVisible","params","pageNum","pageSize","username","phone","email","rules","newPass","required","message","trigger","min","max","created","load","methods","changeStatus","row","id","$notify","success","put","then","res","code","msg","handleChangePass","stringify","savePass","$refs","validate","valid","remove","$router","push","error","list","reset","handleCurrentChange","del","delete"],"sources":["src/views/admin/List.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!--  搜索表单  -->\r\n    <div style=\"margin-bottom: 20px\">\r\n      <el-input style=\"width: 240px\" placeholder=\"请输入用户名\" v-model=\"params.username\"></el-input>\r\n      <el-input style=\"width: 240px;margin-left:5px; \" placeholder=\"请输入电话\" v-model=\"params.phone\"></el-input>\r\n      <el-input style=\"width: 240px;margin-left:5px; \" placeholder=\"请输email\" v-model=\"params.email\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"> <i class=\"el-icon-search\"></i>搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"> <i class=\"el-icon-refresh\"></i>重置</el-button>\r\n    </div>\r\n\r\n    <el-table :data=\"tableDate\" stripe>\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"username\" label=\"用户名\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"电话\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"email\" label=\"email\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"createtime\" label=\"创建时间\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"updatetime\" label=\"更新时间\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"status\" label=\"状态\" width=\"100\" >\r\n        <template v-slot=\"scope\">\r\n          <el-switch\r\n              v-model=\"scope.row.status\"\r\n              @change=\"changeStatus(scope.row)\"\r\n              active-color=\"#13ce66\"\r\n              inactive-color=\"#ff4949\">\r\n          </el-switch>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template v-slot=\"scope\">\r\n<!--          scope.row就是当前行-->\r\n          <el-button type=\"primary\" @click=\"$router.push('/editAdmin?id=' + scope.row.id)\">编辑</el-button>\r\n          <el-popconfirm\r\n              style=\"margin-left: 4px\"\r\n              title=\"你确定删除这行数据吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n          <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"handleChangePass(scope.row)\">修改密码</el-button>\r\n        </template>\r\n      </el-table-column>\r\n\r\n    </el-table>\r\n\r\n    <!--  分页  -->\r\n    <div style=\"margin-top: 20px\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"修改密码\" :visible.sync=\"dialogFormVisible\">\r\n      <el-form :model=\"form\" label-width=\"100px\" ref=\"formRef\" :rules=\"rules\">\r\n        <el-form-item label=\"新密码\" prop=\"newPass\" >\r\n          <el-input v-model=\"form.newPass\" autocomplete=\"off\" show-password></el-input>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"savePass\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie'\r\nexport default {\r\n  name: 'AdminList',\r\n  data(){\r\n    return{\r\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\r\n      tableDate: [],\r\n      total: 0,\r\n      form: {},\r\n      dialogFormVisible: false,\r\n\r\n      params:{\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        username: '',\r\n        phone: '',\r\n        email: ''\r\n      },\r\n      rules: {\r\n        newPass:[\r\n          {required: true, message: '请输入新密码', trigger: 'blur'},\r\n          {min: 3, max: 10, message:'长度在3到10个字符', trigger: 'blur'}\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    changeStatus(row){\r\n      if(this.admin.id===row.id){\r\n        this.$notify.success(\"你的操作不合法\")\r\n        return\r\n      }\r\n      request.put('/admin/update',row).then(res => {\r\n        if (res.code === '200'){\r\n          this.$notify.success('操作成功')\r\n          this.load()\r\n        }else{\r\n          this.$notify.success(res.msg)\r\n        }\r\n\r\n      })\r\n    },\r\n    handleChangePass(row){\r\n      this.form =JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n    },\r\n    savePass(){\r\n      this.$refs['formRef'].validate((valid) => {\r\n        if (valid) {\r\n          request.put('/admin/password',this.form).then(res =>{\r\n            if(res.code === '200'){\r\n              this.$notify.success(\"修改成功\")\r\n              if(this.form.id=== this.admin.id){ //修改当前用户的ID等于用户的ID，重新登录\r\n                Cookies.remove('admin')\r\n                this.$router.push('/login')\r\n              }else {\r\n                this.load()\r\n                this.dialogFormVisible=false\r\n              }\r\n            } else {\r\n              this.$notify.error(\"修改失败\")\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    load(){\r\n      // fetch('http://localhost:9090/admin/list').then(res => res.json()).then(res =>{\r\n      //   console.log(res)\r\n      //   this.tableDate=res\r\n      // })\r\n\r\n      request.get('/admin/page',{\r\n        params: this.params\r\n      }).then(res =>{\r\n        if(res.code === '200'){\r\n          this.tableDate = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.params={\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        username: '',\r\n        phone: '',\r\n        email: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      //点击分页按钮触发\r\n      this.params.pageNum=pageNum\r\n      this.load()\r\n    },\r\n    del(id){\r\n      request.delete(\"/admin/delete/\" +id).then(res =>{\r\n        if(res.code === '200'){\r\n          this.$notify.success(\"删除成功\")\r\n          this.load()\r\n        }else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AA0EA,OAAAA,OAAA;AACA,OAAAC,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA,EAAAH,OAAA,CAAAI,GAAA,YAAAC,IAAA,CAAAC,KAAA,CAAAN,OAAA,CAAAI,GAAA;MACAG,SAAA;MACAC,KAAA;MACAC,IAAA;MACAC,iBAAA;MAEAC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAC,QAAA;QACAC,KAAA;QACAC,KAAA;MACA;MACAC,KAAA;QACAC,OAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAAA;UAAAC,GAAA;UAAAH,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAG,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,aAAAC,GAAA;MACA,SAAAzB,KAAA,CAAA0B,EAAA,KAAAD,GAAA,CAAAC,EAAA;QACA,KAAAC,OAAA,CAAAC,OAAA;QACA;MACA;MACAhC,OAAA,CAAAiC,GAAA,kBAAAJ,GAAA,EAAAK,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAL,OAAA,CAAAC,OAAA;UACA,KAAAN,IAAA;QACA;UACA,KAAAK,OAAA,CAAAC,OAAA,CAAAG,GAAA,CAAAE,GAAA;QACA;MAEA;IACA;IACAC,iBAAAT,GAAA;MACA,KAAAnB,IAAA,GAAAJ,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAiC,SAAA,CAAAV,GAAA;MACA,KAAAlB,iBAAA;IACA;IACA6B,SAAA;MACA,KAAAC,KAAA,YAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA3C,OAAA,CAAAiC,GAAA,yBAAAvB,IAAA,EAAAwB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cACA,KAAAL,OAAA,CAAAC,OAAA;cACA,SAAAtB,IAAA,CAAAoB,EAAA,UAAA1B,KAAA,CAAA0B,EAAA;gBAAA;gBACA7B,OAAA,CAAA2C,MAAA;gBACA,KAAAC,OAAA,CAAAC,IAAA;cACA;gBACA,KAAApB,IAAA;gBACA,KAAAf,iBAAA;cACA;YACA;cACA,KAAAoB,OAAA,CAAAgB,KAAA;YACA;UACA;QACA;MACA;IACA;IACArB,KAAA;MACA;MACA;MACA;MACA;;MAEA1B,OAAA,CAAAK,GAAA;QACAO,MAAA,OAAAA;MACA,GAAAsB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAA5B,SAAA,GAAA2B,GAAA,CAAAhC,IAAA,CAAA6C,IAAA;UACA,KAAAvC,KAAA,GAAA0B,GAAA,CAAAhC,IAAA,CAAAM,KAAA;QACA;MACA;IACA;IACAwC,MAAA;MACA,KAAArC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAC,QAAA;QACAC,KAAA;QACAC,KAAA;MACA;MACA,KAAAS,IAAA;IACA;IACAwB,oBAAArC,OAAA;MACA;MACA,KAAAD,MAAA,CAAAC,OAAA,GAAAA,OAAA;MACA,KAAAa,IAAA;IACA;IACAyB,IAAArB,EAAA;MACA9B,OAAA,CAAAoD,MAAA,oBAAAtB,EAAA,EAAAI,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAL,OAAA,CAAAC,OAAA;UACA,KAAAN,IAAA;QACA;UACA,KAAAK,OAAA,CAAAgB,KAAA,CAAAZ,GAAA,CAAAE,GAAA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sour