{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"AddUser\",\n  data() {\n    return {\n      form: {\n        sex: '男'\n      },\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入活动名称',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 5,\n          message: '长度在 3 到 5 个字符',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    save() {\n      request.post('/user/save', this.form).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('新增成功');\n          this.form = {\n            sex: '男'\n          };\n        } else {\n          this.$notify.success(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","form","sex","rules","required","message","trigger","min","max","methods","save","post","then","res","code","$notify","success","msg"],"sources":["src/views/user/AddUser.vue"],"sourcesContent":["<template>\r\n <div style=\"width: 80%\">\r\n   <div style=\"margin-bottom: 30px\">新增用户</div>\r\n   <el-form :inline=\"true\" :model=\"form\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\">\r\n     <el-form-item label=\"姓名\" >\r\n       <el-input v-model=\"form.name\" placeholder=\"请输入姓名\"></el-input>\r\n     </el-form-item>\r\n     <el-form-item label=\"年龄\">\r\n       <el-input v-model=\"form.age\" placeholder=\"请输入年龄\"></el-input>\r\n     </el-form-item>\r\n     <el-form-item label=\"性别\">\r\n       <el-radio v-model=\"form.sex\" label=\"男\">男</el-radio>\r\n       <el-radio v-model=\"form.sex\" label=\"女\">女</el-radio>\r\n     </el-form-item>\r\n     <el-form-item label=\"联系方式\">\r\n       <el-input v-model=\"form.phone\" placeholder=\"请输入联系方式\"></el-input>\r\n     </el-form-item>\r\n     <el-form-item label=\"地址\">\r\n       <el-input v-model=\"form.address\" placeholder=\"请输入地址\"></el-input>\r\n     </el-form-item>\r\n   </el-form>\r\n\r\n   <div style=\"text-align: center; margin-top: 30px\">\r\n     <el-button type=\"primary\" @click=\"save\" size=\"medium\">提交</el-button>\r\n<!--     <el-button type=\"danger\">返回列表</el-button>-->\r\n   </div>\r\n </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nexport default {\r\n  name: \"AddUser\",\r\n  data(){\r\n    return {\r\n      form: {sex: '男'},\r\n      rules: {\r\n        name: [\r\n          {required: true, message: '请输入活动名称', trigger: 'blur'},\r\n          {min: 3, max: 5, message: '长度在 3 到 5 个字符', trigger: 'blur'}\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    save(){\r\n      request.post('/user/save',this.form).then(res => {\r\n        if (res.code === '200'){\r\n          this.$notify.success('新增成功')\r\n          this.form = { sex: '男'}\r\n        }else{\r\n          this.$notify.success(res.msg)\r\n        }\r\n\r\n      })\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA8BA,OAAAA,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;QAAAC,GAAA;MAAA;MACAC,KAAA;QACAJ,IAAA,GACA;UAAAK,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAAA;UAAAC,GAAA;UAAAH,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAG,OAAA;IACAC,KAAA;MACAZ,OAAA,CAAAa,IAAA,oBAAAV,IAAA,EAAAW,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,OAAA,CAAAC,OAAA;UACA,KAAAf,IAAA;YAAAC,GAAA;UAAA;QACA;UACA,KAAAa,OAAA,CAAAC,OAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MAEA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}