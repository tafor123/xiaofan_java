{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'BorrowList',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableDate: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        book_name: '',\n        book_no: '',\n        user_name: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      // fetch('http://localhost:9090/book/list').then(res => res.json()).then(res =>{\n      //   console.log(res)\n      //   this.tableDate=res\n      // })\n\n      request.get('/borrow/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableDate = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        book_name: '',\n        book_no: '',\n        user_name: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      //点击分页按钮触发\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete(\"/borrow/delete/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$notify.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookies","name","data","admin","get","JSON","parse","tableDate","total","params","pageNum","pageSize","book_name","book_no","user_name","created","load","methods","then","res","code","list","reset","handleCurrentChange","del","id","delete","$notify","success","error","msg"],"sources":["src/views/borrow/List.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!--  搜索表单  -->\r\n    <div style=\"margin-bottom: 20px\">\r\n      <el-input style=\"width: 240px\" placeholder=\"请输入图书名称\" v-model=\"params.book_name\"></el-input>\r\n      <el-input style=\"width: 240px; margin-left: 5px\" placeholder=\"请输入图书标准码\" v-model=\"params.book_no\"></el-input>\r\n      <el-input style=\"width: 240px; margin-left: 5px\" placeholder=\"请输入用户姓名\" v-model=\"params.user_name\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"> <i class=\"el-icon-search\"></i>搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"> <i class=\"el-icon-refresh\"></i>重置</el-button>\r\n    </div>\r\n\r\n    <el-table :data=\"tableDate\" stripe row-key=\"id\"  default-expand-all>\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"book_name\" label=\"图书\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"book_no\" label=\"标准码\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"user_no\" label=\"会员名\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"user_name\" label=\"用户姓名\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"user_phone\" label=\"用户电话\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"score\" label=\"所用积分\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"createtime\" label=\"创建时间\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"updatetime\" label=\"更新时间\" width=\"100\"></el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template v-slot=\"scope\">\r\n<!--          scope.row就是当前行-->\r\n          <el-button type=\"success\" @click=\"$router.push('/editBorrow?id=' + scope.row.id)\">编辑</el-button>\r\n<!--          <el-popconfirm-->\r\n<!--              style=\"margin-left: 4px\"-->\r\n<!--              title=\"你确定删除这行数据吗？\"-->\r\n<!--              @confirm=\"del(scope.row.id)\"-->\r\n<!--          >-->\r\n<!--            <el-button type=\"danger\" slot=\"reference\">删除</el-button>-->\r\n<!--          </el-popconfirm>-->\r\n        </template>\r\n      </el-table-column>\r\n\r\n    </el-table>\r\n\r\n    <!--  分页  -->\r\n    <div style=\"margin-top: 20px\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie'\r\nexport default {\r\n  name: 'BorrowList',\r\n  data(){\r\n    return{\r\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\r\n      tableDate: [],\r\n      total: 0,\r\n      params:{\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        book_name: '',\r\n        book_no: '',\r\n        user_name: ''\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load(){\r\n      // fetch('http://localhost:9090/book/list').then(res => res.json()).then(res =>{\r\n      //   console.log(res)\r\n      //   this.tableDate=res\r\n      // })\r\n\r\n      request.get('/borrow/page',{\r\n        params: this.params\r\n      }).then(res =>{\r\n        if(res.code === '200'){\r\n          this.tableDate = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.params={\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        book_name: '',\r\n        book_no: '',\r\n        user_name: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      //点击分页按钮触发\r\n      this.params.pageNum=pageNum\r\n      this.load()\r\n    },\r\n    del(id){\r\n      request.delete(\"/borrow/delete/\" +id).then(res =>{\r\n        if(res.code === '200'){\r\n          this.$notify.success(\"删除成功\")\r\n          this.load()\r\n        }else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAsDA,OAAAA,OAAA;AACA,OAAAC,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA,EAAAH,OAAA,CAAAI,GAAA,YAAAC,IAAA,CAAAC,KAAA,CAAAN,OAAA,CAAAI,GAAA;MACAG,SAAA;MACAC,KAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAC,SAAA;QACAC,OAAA;QACAC,SAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA;MACA;MACA;MACA;;MAEAjB,OAAA,CAAAK,GAAA;QACAK,MAAA,OAAAA;MACA,GAAAS,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAb,SAAA,GAAAY,GAAA,CAAAjB,IAAA,CAAAmB,IAAA;UACA,KAAAb,KAAA,GAAAW,GAAA,CAAAjB,IAAA,CAAAM,KAAA;QACA;MACA;IACA;IACAc,MAAA;MACA,KAAAb,MAAA;QACAC,OAAA;QACAC,QAAA;QACAC,SAAA;QACAC,OAAA;QACAC,SAAA;MACA;MACA,KAAAE,IAAA;IACA;IACAO,oBAAAb,OAAA;MACA;MACA,KAAAD,MAAA,CAAAC,OAAA,GAAAA,OAAA;MACA,KAAAM,IAAA;IACA;IACAQ,IAAAC,EAAA;MACA1B,OAAA,CAAA2B,MAAA,qBAAAD,EAAA,EAAAP,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAO,OAAA,CAAAC,OAAA;UACA,KAAAZ,IAAA;QACA;UACA,KAAAW,OAAA,CAAAE,KAAA,CAAAV,GAAA,CAAAW,GAAA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}