rimary\" @click=\"save\" size=\"medium\">提交</el-button>\r\n<!--     <el-button type=\"danger\">返回列表</el-button>-->\r\n   </div>\r\n </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nexport default {\r\n  name: \"AddUser\",\r\n  data(){\r\n    return {\r\n      form: {}\r\n    }\r\n  },\r\n  created(){\r\n    const id = this.$route.query.id\r\n    request.get(\"/user/\" + id).then(res => {\r\n      this.form =res.data\r\n    })\r\n  },\r\n  methods: {\r\n    save(){\r\n      request.put('/user/update',this.form).then(res => {\r\n        if (res.code === '200'){\r\n          this.$notify.success('更新成功')\r\n          this.$router.push(\"/user\")\r\n        }else{\r\n          this.$notify.success(res.msg)\r\n        }\r\n\r\n      })\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AAgCA,OAAAA,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,MAAAC,EAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAF,EAAA;IACAL,OAAA,CAAAQ,GAAA,YAAAH,EAAA,EAAAI,IAAA,CAAAC,GAAA;MACA,KAAAP,IAAA,GAAAO,GAAA,CAAAR,IAAA;IACA;EACA;EACAS,OAAA;IACAC,KAAA;MACAZ,OAAA,CAAAa,GAAA,sBAAAV,IAAA,EAAAM,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAI,IAAA;UACA,KAAAC,OAAA,CAAAC,OAAA;UACA,KAAAC,OAAA,CAAAC,IAAA;QACA;UACA,KAAAH,OAAA,CAAAC,OAAA,CAAAN,GAAA,CAAAS,GAAA;QACA;MAEA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}